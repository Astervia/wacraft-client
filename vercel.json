{
    "redirects": [
        {
            "source": "/",
            "destination": "/en/",
            "permanent": false
        }
    ],
    "rewrites": [
        {
            "source": "/en/:path*",
            "destination": "/en/index.html"
        },
        {
            "source": "/pt-BR/:path*",
            "destination": "/pt-BR/index.html"
        }
    ]
}
